@model LojaComputadores.Models.CatalogoComputadores.ComputadorCadastroModel

<div class="container">
    <div class="page-header clearfix detailHeading">
        <h2 class="text-muted">Cadastrar Computador</h2>
    </div>
</div>

<div class="jumbotron">
    <div class="form-group">
        <form action="ComputadorCadastroModel" method="post" id="form-post">
            <div class="col-xs-6">
                <label for="Nome">Nome</label>
                <input class="form-control" id="Nome" type="text" />
            </div>
            <div class="col-xs-6">
                <label for="HD">HD</label>
                <input class="form-control" id="HD" type="text" />
            </div>
            <div class="col-xs-6">
                <label for="Ram">Memória Ram</label>
                <input class="form-control" id="Ram" type="text" />
            </div>
            <div class="col-xs-6">
                <label for="PlacaMae">Placa Mãe</label>
                <input class="form-control" id="PlacaMae" type="text" />
            </div>
            <div class="col-xs-6">
                <label for="PlacaVideo">Placa de Vídeo</label>
                <input class="form-control" id="PlacaVideo" type="text" />
            </div>
            <div class="col-xs-6">
                <label for="Processador">Processador</label>
                <input class="form-control" id="Processador" type="text" />
            </div>
            <div class="col-xs-6">
                <label for="Preco">Preço</label>
                <input class="form-control" id="Preco" type="number" />
            </div>
            <div>
                <button type="submit">Cadastrar</button>
            </div>
        </form>
    </div>
</div>
